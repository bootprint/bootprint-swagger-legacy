{
  "name": "bootprint-swagger",
  "version": "0.14.0",
  "description": "Bootprint-module to render swagger specifications",
  "repository": {
    "type": "git",
    "url": "https://github.com/bootprint/bootprint-swagger.git"
  },
  "author": {
    "name": "Nils Knappmeier",
    "email": "npm@knappi.org",
    "url": "https://github.com/nknapp"
  },
  "bugs": {
    "url": "https://github.com/bootprint/bootprint-swagger/issues"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "pretest": "standard --version || npm -g install standard",
    "test": "mocha && standard",
    "preformat": "standard --version || npm -g install standard",
    "format": "standard --format"
  },
  "dependencies": {
    "bootprint-openapi": "<1.0.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "ghooks": "^1.0.3",
    "mocha": "^2.2.5",
    "thoughtful-release": "^0.3.0"
  },
  "standard": {
    "ignore": [
      "spec/fixtures/**"
    ]
  },
  "files": [
    "index.js",
    "lib",
    "bin"
  ],
  "config": {
    "ghooks": {
      "pre-commit": "standard"
    }
  },
  "keywords": [
    "swagger",
    "html",
    "swagger-to-html"
